<?xml version="1.0" encoding="UTF-8"?>
<module>
	<inherits name="com.google.gwt.dev.jjs.intrinsic.Intrinsic" />
	<inherits name="com.google.gwt.emul.Emulation" />
	<inherits name="com.google.gwt.core.CompilerParameters" />
	<inherits name="com.google.gwt.core.EmulateJsStack" />
	<inherits name="com.google.gwt.json.JSON" />

	<define-linker name="pmt" class="com.pmt.gwt.PMTScriptLinker" />
	<add-linker name="pmt" />

	<generate-with class="com.pmt.gwt.reflection.IntrospectorGenerator">
		<when-type-assignable class="com.pmt.sys.reflection.Introspector" />
	</generate-with>

	<source path='database' />
	<source path='network' />
	<source path='sys' />
	<source path='wrap' />
</module>
